<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenunActivity">
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_freepost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="자유게시판"
            android:textSize="24dp"
            android:backgroundTint="#FF914D"
            />

        <Button
            android:id="@+id/btn_phexercise"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PH운동추천"
            android:textSize="24dp"
            android:backgroundTint="#FF914D"/>

        <Button
            android:id="@+id/btn_calendar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="캘린더"
            android:textSize="24dp"
            android:backgroundTint="#FF914D"/>

        <Button
            android:id="@+id/btn_inbody"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="인바디 등록"
            android:textSize="24dp"
            android:backgroundTint="#FF914D"/>

        <Button
            android:id="@+id/btn_exercise"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="운동"
            android:textSize="24dp"
            android:backgroundTint="#FF914D"/>

        <Button
            android:id="@+id/btn_diet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="식단"
            android:textSize="24dp"
            android:backgroundTint="#FF914D"/>

        <Button
            android:id="@+id/back_button_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="#FF914D"
            android:text="뒤로"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>